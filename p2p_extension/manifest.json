{
  "manifest_version": 3,
  "name": "P2P Chat & Call Extension",
  "version": "1.0",
  "description": "Chat, File, Call & Screen Share with Flutter app",
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
